@model Marketplace.Admin.ViewModels.ServiceViewModel
@using Marketplace.Admin.Utils;

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ActionTitle = "Add service";
}

@using (Html.BeginForm("Create", "Services", FormMethod.Post, new {@id="serviceForm", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.Partial("_Service", @Model)

}

@section Scripts {

    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/services")
}
